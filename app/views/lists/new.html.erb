<div class="container">
  <div class="new">
  	<h1>Create my list</h1>
    <div class="form">
      <%= simple_form_for(@list) do |f| %>
      <%= f.input :name %>
      <%= f.association :movies %>
      <%= f.button :submit, class: 'btn btn-success' %>
      <% end %>
	  </div>
  </div>
   <%= link_to 'Home', lists_path, class: 'btn btn-primary' %>
</div>

